{
    "name": "metago",
    "displayName": "MetaGo",
    "description": "vscode meta goto. jump, navigation, goto",
    "icon": "images/metago.png",
    "version": "1.0.1",
    "publisher": "metaseed",
    "homepage": "https://github.com/metaseed/metaGo",
    "license": "MIT",
    "keywords": [
        "jump",
        "jumpy",
        "goto",
        "shortcut",
        "move",
        "cursor",
        "navigation",
        "EasyMotion",
        "metaGo",
        "center screen",
        "focus screen",
        "space jumper"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/metaseed/metaGo.git"
    },
    "bugs": {
        "url": "https://github.com/metaseed/metaGo/issues",
        "email": "metaseed@gmail.com"
    },
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.metaGo",
                "title": "metaGo:GoTo"
            },
            {
                "command": "extension.metaGo.selection",
                "title": "metaGo:selection"
            }
        ],
        "keybindings": [
            {
                "command": "extension.metaGo.input.stop",
                "key": "escape",
                "when": "editorTextFocus && metaGoInput"
            },
            {
                "command": "extension.metaGo",
                "key": "ctrl+l",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.metaGo.selection",
                "key": "ctrl+shift+l",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.metaGo.centerEditor",
                "key": "shift+space",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+home",
                "command": "spaceBlockJumper.moveUp",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+shift+home",
                "command": "spaceBlockJumper.selectUp",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+end",
                "command": "spaceBlockJumper.moveDown",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+shift+end",
                "command": "spaceBlockJumper.selectDown",
                "when": "editorTextFocus"
            }
        ],
        "configuration": {
            "title": "metaseed metaGo",
            "type": "object",
            "properties": {
                "metaGo.placeholder.backgroundColor": {
                    "type": "string",
                    "default": "lime,yellow"
                },
                "metaGo.placeholder.color": {
                    "type": "string",
                    "default": "black"
                },
                "metaGo.placeholder.stroke": {
                    "type": "string",
                    "default": "black"
                },
                "metaGo.placeholder.width": {
                    "type": "number",
                    "default": 9
                },
                "metaGo.placeholder.height": {
                    "type": "number",
                    "default": 15
                },
                "metaGo.placeholder.fontSize": {
                    "type": "number",
                    "default": 13
                },
                "metaGo.placeholder.x": {
                    "type": "number",
                    "default": 1
                },
                "metaGo.placeholder.y": {
                    "type": "number",
                    "default": 10
                },
                "metaGo.placeholder.fontWeight": {
                    "type": "string",
                    "default": "normal"
                },
                "metaGo.placeholder.fontFamily": {
                    "type": "string",
                    "default": "Consolas"
                },
                "metaGo.placeholder.upperCase": {
                    "type": "boolean",
                    "default": false
                },
                "metaGo.finder.findAllMode": {
                    "type": "string",
                    "default": "on"
                },
                "metaGo.finder.wordSeparatorPattern": {
                    "type": "string",
                    "default": "[ ,-.{_(\"'<\\/[+]"
                },
                "metaGo.finder.range": {
                    "type": "number",
                    "default": 40
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.2.2",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "@types/lodash": "^4.14.37",
        "@types/xmlbuilder": "0.0.28",
        "lodash": "^4.16.4"
    }
}