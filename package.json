{
    "name": "metago",
    "displayName": "MetaGo",
    "description": "vscode meta goto. jump, navigation, goto",
    "icon": "images/metago.png",
    "version": "1.0.1",
    "publisher": "metaseed",
    "homepage": "https://github.com/metaseed/metaGo",
    "license": "MIT",
    "keywords": [
        "jump",
        "jumpy",
        "metago",
        "goto",
        "move",
        "cursor",
        "navagition",
        "EasyMotion",
        "metaGo"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/metaseed/metaGo.git"
    },
    "bugs": {
        "url": "https://github.com/metaseed/metaGo/issues",
        "email": "metaseed@gmail.com"
    },
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.metaGo",
                "title": "metaGo:GoTo"
            },
            {
                "command": "extension.metaGo.selection",
                "title": "metaGo:selection"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "keybindings": [
        {
            "command": "extension.metaGo.input.stop",
            "key": "escape",
            "when": "editorTextFocus && metaGoInput"
        }
    ],
    "configuration": {
        "title": "metaseed metaGo",
        "type": "object",
        "properties": {
            "metaGo.placeholder.backgroundColor": {
                "type": "string",
                "default": "red"
            },
            "metaGo.placeholder.color": {
                "type": "string",
                "default": "black"
            },
            "metaGo.placeholder.border": {
                "type": "string",
                "default": "dotted thin black"
            },
            "metaGo.placeholder.width": {
                "type": "number",
                "default": 12
            },
            "metaGo.placeholder.height": {
                "type": "number",
                "default": 14
            },
            "metaGo.placeholder.fontSize": {
                "type": "number",
                "default": 14
            },
            "metaGo.placeholder.x": {
                "type": "number",
                "default": 2
            },
            "metaGo.placeholder.y": {
                "type": "number",
                "default": 12
            },
            "metaGo.placeholder.fontWeight": {
                "type": "string",
                "default": "normal"
            },
            "metaGo.placeholder.fontFamily": {
                "type": "string",
                "default": "Consolas"
            },
            "metaGo.placeholder.upperCase": {
                "type": "boolean",
                "default": false
            },
            "metaGo.finder.pattern": {
                "type": "string",
                "default": "[ ,-.{_(\"'<\\/[+]"
            },
            "metaGo.finder.range": {
                "type": "number",
                "default": 40
            }
        }
    },
    "devDependencies": {
        "typescript": "^2.2.2",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "@types/lodash": "^4.14.37",
        "@types/xmlbuilder": "0.0.28",
        "lodash": "^4.16.4"
    }
}